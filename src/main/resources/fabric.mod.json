{
  "schemaVersion": 1,
  "id": "worldy",
  "version": "${version}",
  "name": "Worldy",
  "description": "A simple fabric mod that enhances your experience on WorldMC",
  "authors": ["Crushed_Cookie"],
  "contributors": [],
  "contact": {
    "website": "https://modrinth.com/mod/worldy",
    "issues": "https://github.com/WMC-Toolkit-Development/Worldy/issues"
  },
  "license": "MIT",
  "icon": "assets/worldy/icon.png",
  "environment": "client",
  "custom": {
    "modmenu": {
      "update-checker": true,
      "modmenu.icon": "assets/worldy/icon.png"
    }
  },
  "entrypoints": {
    "fabric-datagen": [
      "jinzo.worldy.client.WorldyDataGenerator"
    ],
    "client": [
      "jinzo.worldy.client.WorldyClient"
    ],
    "main": [
      "jinzo.worldy.Worldy"
    ],
    "modmenu": [
      "jinzo.worldy.client.utils.ModMenu"
    ]
  },
  "mixins": [
    "worldy.mixins.json",
    {
      "config": "worldy.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=1.21.10 <1.21.12",
    "modmenu": "*",
    "cloth-config": "*"
  }
}
